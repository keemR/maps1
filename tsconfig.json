{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["chrome-types"]
  },
  "include": [
    "*.js",
    "*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}